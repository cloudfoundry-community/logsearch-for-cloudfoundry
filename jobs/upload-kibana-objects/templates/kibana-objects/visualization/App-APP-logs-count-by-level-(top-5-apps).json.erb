<% require 'json' %>
<% if(p('cloudfoundry.firehose_events').include? 'LogMessage') %>
{
  "attributes": {
    "title": "App APP: logs count by level (top 5 apps)",
    "visState": "<%= JSON.parse(<<'ENDOFFJSON').to_json.gsub(/"/) { '\"' }
      {
        "type": "histogram",
        "params": {
          "shareYAxis": true,
          "addTooltip": true,
          "addLegend": true,
          "scale": "linear",
          "mode": "stacked",
          "times": [],
          "addTimeMarker": false,
          "defaultYExtents": false,
          "setYExtents": false,
          "yAxis": {}
        },
        "aggs": [
          {
            "id": "1",
            "type": "count",
            "schema": "metric",
            "params": {}
          },
          {
            "id": "2",
            "type": "terms",
            "schema": "segment",
            "params": {
              "field": "@level",
              "size": 100,
              "order": "desc",
              "orderBy": "1"
            }
          },
          {
            "id": "3",
            "type": "terms",
            "schema": "split",
            "params": {
              "field": "@cf.app",
              "size": 5,
              "order": "desc",
