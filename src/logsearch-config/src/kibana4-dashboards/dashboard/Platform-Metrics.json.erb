<% require 'json' %>
{
  "title": "Platform - Metrics",
  "hits": 0,
  "description": "",
  "panelsJSON": "<%= JSON.parse(<<'ENDOFFJSON').to_json.gsub(/"/) { '\"' }
    [
      {
        "col": 11,
        "id": "metric-keys",
        "row": 3,
        "size_x": 2,
        "size_y": 5,
        "type": "visualization"
      },
      {
        "col": 1,
        "id": "metric-median-of-value_int",
        "row": 1,
        "size_x": 10,
        "size_y": 4,
        "type": "visualization"
      },
      {
        "col": 11,
        "id": "metric-components",
        "row": 1,
        "size_x": 2,
        "size_y": 2,
        "type": "visualization"
      },
      {
        "col": 1,
        "columns": [
          "@source.name",
          "@message"
        ],
        "id": "metric",
        "row": 5,
        "size_x": 10,
        "size_y": 3,
        "sort": [
          "@timestamp",
          "desc"
        ],
        "type": "search"
      }
    ]
ENDOFFJSON
%>",
  "optionsJSON": "{\"darkTheme\":false}",
  "version": 1,
  "timeRestore": false,
  "kibanaSavedObjectMeta": {
    "searchSourceJSON": "<%= JSON.parse(<<'ENDOFFJSON').to_json.gsub(/"/) { '\"' }
    {
      "filter": [
        {
          "meta": {
            "disabled": true,
            "index": "[logs-platform-]YYYY.MM.DD",
            "key": "metric.key",
            "negate": false,
            "value": "healthy"
          },
          "query": {
            "match": {
              "metric.key": {
                "query": "healthy",
                "type": "phrase"
              }
            }
          }
        },
        {
          "meta": {
            "index": "[logs-platform-]YYYY.MM.DD",
            "key": "metric.key",
            "value": "cpu_load_avg",
            "disabled": true,
            "negate": false
          },
          "query": {
            "match": {
              "metric.key": {
                "query": "cpu_load_avg",
                "type": "phrase"
              }
            }
          }
        },
        {
          "query": {
            "query_string": {
              "analyze_wildcard": true,
              "query": "*"
            }
          }
        }
      ]
    }
ENDOFFJSON
%>"
  }
}
